<view id="my">
<!-- 登录页 -->
  <view class="header-container">
      <image class='header-bg' src = '../../images/banner.png'></image>
      <view class='userContent'>
        <image bindtap='changePhotoImg' 
        class="header-avatar" 
        src="{{obj.avatarUrl==undeifined?'../../images/dl.png':obj.avatarUrl}}" ></image>    
        <button open-type="getUserInfo" 
                bindgetuserinfo="handle3" 
                class="header-nickname">
                {{obj.nickName==undeifined?"请登录":obj.nickName}}</button>
      </view>
</view>   
  <!-- 我的订单 -->
  <view>
    <view class="myorder">
      <text class="myorder-left">我的订单</text>
      <text class="myorder-right">查看订单></text>
    </view>
  </view>
  <!-- icon图标 -->
  <view class="icon-big">
    <view class="icon-1">
      <text class="iconfont icon-fukuan" style="font-size:60rpx;color:#666666;"></text>
      <text style="font-size:26rpx;color:#666666;">待付款</text>
    </view>
    <view class="icon-1">
      <text class="iconfont icon-daifahuo" style="font-size:60rpx;color:#666666;"></text>
      <text style="font-size:26rpx;color:#666666;">待发货</text>
    </view>
    <view class="icon-1">
      <text class="iconfont icon-daifahuo1" style="font-size:60rpx;color:#666666;"></text>
      <text style="font-size:26rpx;color:#666666;">待收货</text>
    </view>
    <view class="icon-1">
      <text class="iconfont icon-pingjia" style="font-size:60rpx;color:#666666;"></text>
      <text style="font-size:26rpx;color:#666666;">待评价</text>
    </view>
    <view class="icon-1">
      <text class="iconfont icon-tuihuo" style="font-size:60rpx;color:#666666;"></text>
      <text style="font-size:26rpx;color:#666666;">退货</text>
    </view>
  </view>
  <!-- 哆米工具 -->
  <view>
    <text class="gongju">哆米工具</text>
    <view class="gongju-flex">
      <view class="icon-2">
        <text class="iconfont icon-wodeshoucang" style="font-size:60rpx;color:#ff103ad4;"></text>
        <text style="font-size:26rpx;color:#666666;">我的收藏</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-wodezuji" style="font-size:60rpx;color:#ff103ad4;"></text>
        <text style="font-size:26rpx;color:#666666;">浏览足迹</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-youhuiquan" style="font-size:60rpx;color:#ff103ad4;"></text>
        <text style="font-size:26rpx;color:#666666;">优惠券</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-qiandao-" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">每日签到</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-dizhi" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">收货地址</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-pingjia" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">我的评论</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon--xiaoxi" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">我的消息</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-lingdang" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">帮助中心</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-yaoqinghaoyoupengyou2" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">邀请好友</text>
      </view>
      <view class="icon-2">
        <text class="iconfont icon-yijianfankui" style="font-size:60rpx;color:#f6b350;"></text>
        <text style="font-size:26rpx;color:#666666;">意见反馈</text>
      </view>
    </view>
  </view>
  <!-- 登录按钮 -->
  <!-- <button open-type="getUserInfo" bindgetuserinfo="handle3" class="bor bor_1">确认</button> -->
</view>